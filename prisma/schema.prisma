generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model plugin {
  plugin_id Int    @id @default(autoincrement())
  name      String @db.VarChar(255)
  repo_url  String
}
