generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model nvim_plugin {
  nvim_plugin_id       Int    @id(map: "plugin_pkey") @default(autoincrement())
  nvim_plugin_name     String @db.VarChar(255)
  nvim_plugin_repo_url String
}
